#!/bin/bash
#PBS -q long
#PBS -l nodes=1:ppn=1
#PBS -l mem=8gb,vmem=8gb

## Load .bashrc
source $HOME/.bashrc
module purge
module load gnu/10.2.0
module load python/3.8-anaconda

## Loop
wpath="/home/ajeze/ERA5/t2m"
for ((year = 1950 ; year < 2022 ; year++ ))
do
	python3 $wpath/ERA5_t2m.py $year
done

